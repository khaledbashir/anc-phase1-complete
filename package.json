    "private": true,
    "packageManager": "pnpm@10.28.2",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest",
        "dev": "next dev --port 3003",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "extract:intel": "node scripts/extract-excel-intel.mjs --input ./specimens --out ./out",
        "test:pricing-parser": "npx tsx scripts/test-pricing-parser.ts",
        "test:pricing-parser:real": "npx tsx scripts/test-pricing-parser-real.ts",
        "test:pricing-parser:golden": "npx tsx scripts/test-pricing-parser-golden.ts",
        "test:pricing-pdf:golden": "npx tsx scripts/test-pricing-pdf-golden.ts",
        "test:pdf:engine-parity": "npx tsx scripts/test-pdf-engine-parity.ts",
        "test:pricing-parser:all": "pnpm run test:pricing-parser && pnpm run test:pricing-parser:real",
        "test:pricing:gate": "pnpm run test:pricing-parser && pnpm run test:pricing-parser:real && pnpm run test:pricing-parser:golden && pnpm run test:pricing-pdf:golden",
        "postinstall": "node -e \"const fs=require('fs');const cp=require('child_process');if(fs.existsSync('prisma/schema.prisma')||fs.existsSync('schema.prisma')){cp.execSync('npx prisma generate',{stdio:'inherit'});}else{console.log('postinstall: prisma schema not found, skipping prisma generate');}\"",
        "analyze": "cross-env ANALYZE=true next build",
        "download-pdfs": "npx tsx scripts/download-all-pdfs.ts"
    },
    "prisma": {
        "seed": "npx tsx prisma/seed.ts"
    },
